{
  "extends": ["plugin:@fluentui/eslint-plugin/react"],
  "root": true,
  "rules": {
    "@typescript-eslint/no-explicit-any": "off",
    "@typescript-eslint/jsx-no-bind": "off",
    "import/no-extraneous-dependencies": ["error", { "packageDir": [".", "../.."] }],
    "overrides": [
      {
        "files": ["src/**/*.{tsx,ts}"],
        "rules": {
          "@typescript-eslint/no-deprecated": "off"
        }
      }
    ]
  }
}
